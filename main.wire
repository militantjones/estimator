            <alpha value="1" time=".2" target="ACCESSORIES-PAGER-BOX/pager/accessory[var:prev-roofdeck-index]/accessory-btn/test/button" />
            <assign property="var:prev-roofdeck-index" value="[var:cur-roofdeck-index]" />
