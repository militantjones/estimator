      <sync>
        <if lhs="[object:LAYERS/input.text]" operator="gt" rhs="0">
          <if lhs="[object:TYPE/input.text]" operator="ne" rhs="">
            <translate xpos="101%" curve="linear" target="STEP-NAVBLOCK" />
            <set onchange="null" target="NAVBLOCK" />
          </if>
        </if>
      </sync>
    </action>

