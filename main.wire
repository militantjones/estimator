<!-- 					<alert message="var:accessoriesObject is [var:accessoriesObject] var:adjustmentsObject is [var:adjustmentsObject]" /> -->
					<play action="prepPlywood" />
				</else>
			</if>
		</action>

		<action name="formatAdjustments" >
