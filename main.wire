        </loop>
        <play action="blur" />
        <alpha value="1" time=".25" target="SHINGLECOLOR" />
        <translate valign="center" time=".25" curve="easeout" target="SHINGLECOLOR" />
        <assign property="var:curtainaction" value="colordismiss" />
        <else>
